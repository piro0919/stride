    "prettier": "prettier --write .",
    "secretlint": "secretlint '**/*'",
    "start": "next start",
    "studio": "npx prisma studio",
    "studio:dev": "dotenv -e .env.development.local -- npm run studio",
    "studio:prod": "dotenv -e .env.production.local -- npm run studio",
